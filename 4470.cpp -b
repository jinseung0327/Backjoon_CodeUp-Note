#include <iostream>
#include <limits>
#include <string>

using namespace std;

int main()
{
    int n;
    cin >> n;
    cin.ignore(numeric_limits<streamsize>::max(), '\n');
    string str;

    for (int i = 1; i <= n; i++)
    {
        getline(cin, str);
        cout << i << ". " << str << endl;
    }

    return 0;
}
